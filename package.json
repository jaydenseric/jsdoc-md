{
  "name": "jsdoc-md",
  "version": "11.0.2",
  "description": "A CLI and equivalent JS API to analyze source JSDoc and generate documentation under a given heading in a markdown file (such as readme.md).",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/jsdoc-md",
  "homepage": "https://github.com/jaydenseric/jsdoc-md#readme",
  "bugs": "https://github.com/jaydenseric/jsdoc-md/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "jsdoc",
    "md",
    "markdown",
    "readme",
    "babel",
    "remark",
    "unified",
    "esm",
    "mjs"
  ],
  "files": [
    "cli",
    "private",
    "public"
  ],
  "sideEffects": false,
  "main": "./public/index.mjs",
  "exports": {
    ".": "./public/index.mjs",
    "./public/*": "./public/*",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "bin": {
    "jsdoc-md": "cli/jsdoc-md.mjs"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >= 16.0.0"
  },
  "peerDependencies": {
    "prettier": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/code-frame": "^7.16.7",
    "@babel/core": "^7.16.12",
    "arg": "^5.0.1",
    "comment-parser": "^1.3.0",
    "doctrine": "^3.0.0",
    "github-slugger": "^1.4.0",
    "globby": "^13.1.0",
    "kleur": "^4.1.4",
    "mdast-util-inject": "^1.1.0",
    "remark-behead": "^3.0.0",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-stringify": "^10.0.2",
    "remark-toc": "^8.0.1",
    "trim-newlines": "^4.0.2",
    "unified": "^10.1.1",
    "unist-util-remove-position": "^4.0.1"
  },
  "devDependencies": {
    "coverage-node": "^6.0.1",
    "disposable-directory": "^5.0.1",
    "eslint": "^8.8.0",
    "eslint-config-env": "^23.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "flatted": "^3.2.5",
    "prettier": "^2.5.1",
    "revertable-globals": "^3.0.0",
    "snapshot-assertion": "^4.0.1",
    "test-director": "^8.0.1"
  },
  "scripts": {
    "jsdoc": "node cli/jsdoc-md.mjs",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:jsdoc && npm run test:api",
    "test:eslint": "eslint --ext mjs,js .",
    "test:prettier": "prettier -c .",
    "test:jsdoc": "node cli/jsdoc-md.mjs -c",
    "test:api": "coverage-node test/index.mjs",
    "prepublishOnly": "npm test"
  }
}
