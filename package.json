{
  "name": "jsdoc-md",
  "version": "1.6.0",
  "description": "A CLI to analyze source JSDoc and generate documentation under a given heading in a markdown file (such as readme.md).",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/jsdoc-md",
  "homepage": "https://github.com/jaydenseric/jsdoc-md#readme",
  "bugs": "https://github.com/jaydenseric/jsdoc-md/issues",
  "keywords": [
    "jsdoc",
    "md",
    "markdown",
    "readme",
    "babel",
    "remark",
    "unified"
  ],
  "files": [
    "lib",
    "index.js",
    "cli.js",
    "!*.test.js"
  ],
  "bin": "cli.js",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "doctrine": "^3.0.0",
    "github-slugger": "^1.2.0",
    "globby": "^8.0.1",
    "mdast-util-inject": "^1.1.0",
    "remark-parse": "^6.0.3",
    "remark-stringify": "^6.0.4",
    "remark-toc": "^5.1.0",
    "unified": "^7.0.2",
    "yargs": "^12.0.4"
  },
  "devDependencies": {
    "circular-json": "^0.5.9",
    "eslint": "^5.9.0",
    "eslint-config-env": "^2.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-import-order-alphabetical": "0.0.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.5",
    "prettier": "^1.15.2",
    "tap": "^12.1.0"
  },
  "scripts": {
    "prepare": "npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:jsdoc": "node cli",
    "prepare:prettier": "prettier readme.md --write",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:js",
    "test:eslint": "eslint .",
    "test:prettier": "prettier '**/*.{json,yml,md}' -l",
    "test:js": "tap ./lib/*.test.js -R spec",
    "prepublishOnly": "npm run prepare && npm test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.{json,yml,md}": "prettier -l"
  },
  "eslintIgnore": [
    "tap-snapshots"
  ],
  "eslintConfig": {
    "extends": [
      "env"
    ],
    "overrides": [
      {
        "files": "*.test.js",
        "rules": {
          "node/no-unpublished-require": "off"
        }
      }
    ]
  },
  "prettier": {
    "proseWrap": "never",
    "singleQuote": true,
    "semi": false
  }
}
